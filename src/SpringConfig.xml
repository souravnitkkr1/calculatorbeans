<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
	<bean id="a" class="com.springcore.Operator">
		<constructor-arg ref="addBean" />
	</bean>
	<bean id="b" class="com.springcore.Operator">
		<constructor-arg ref="subBean" />
	</bean>
	<bean id="c" class="com.springcore.Operator">
		<constructor-arg ref="mulBean" />
	</bean>
	<bean id="d" class="com.springcore.Operator">
		<constructor-arg ref="divBean" />
	</bean>
	<bean id="e" class="com.springcore.Operator">
		<constructor-arg ref="modBean" />
	</bean>
	<bean id="addBean" class="com.springcore.Add"></bean>
	<bean id="subBean" class="com.springcore.Sub"></bean>
	<bean id="mulBean" class="com.springcore.Mul"></bean>
	<bean id="divBean" class="com.springcore.Div"></bean>
	<bean id="modBean" class="com.springcore.Mod"></bean>
</beans>